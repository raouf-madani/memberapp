{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 12",
    "extends": "@tsconfig/node12/tsconfig.json",
    "compilerOptions": {
      "lib": ["es2016","es2019", "es2020.promise", "es2020.bigint", "es2020.string"],
      "module": "commonjs",
      "target": "es2019",
  
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "../wwwroot/app",
        "node_modules/*"
      ]
  }